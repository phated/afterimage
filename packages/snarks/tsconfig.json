{
  "extends": "@projectsophon/tsconfig",
  "include": ["index.ts"],
  "typedocOptions": {
    "entryPoints": ["src"],
    "out": "docs",
    "hideBreadcrumbs": true,
    "readme": "none",
    "disableSources": true,
    "disableOutputCheck": true,
    "excludeExternals": true,
    "listInvalidSymbolLinks": true
  }
}
